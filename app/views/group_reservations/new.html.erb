<%= form_with(model: @group_reservation) do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name, class: 'form-control' %>
  </div>
  <div>
    <%= f.label :check_in_date %>
    <%= f.date_field :check_in_date, class: 'form-control' %>
  </div>
  <div>
    <%= f.label :check_out_date %>
    <%= f.date_field :check_out_date, class: 'form-control' %>
  </div>
  <div>
    <%= f.submit 'Fazer reserva' %>
  </div>
<% end %>
